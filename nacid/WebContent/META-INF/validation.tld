		</attribute>
	</tag>
	<tag>
		<name>dateIntervalValidator</name>
		<tag-class>com.nacid.web.taglib.validation.DateIntervalValidatorTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<name>dateFrom</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>dateTo</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<!-- date format. Example:d.m.yyyy -->
			<name>format</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<!-- date format. of end date (if not specified format is used) Example:d.m.yyyy -->
			<name>endFormat</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<!-- Error message displayed if dateFrom >(=) dateTo -->
			<name>errormessage</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<!-- if true - dateFrom and dateTo must not be equal; if false - equal dates are allowed-->
			<name>dontAllowEquals</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
