	version="2.0">
	<tlib-version>1.2</tlib-version>
	<tag>
		<name>form</name>
		<tag-class>com.nacid.web.taglib.validation.FormTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<name>action</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>method</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>name</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>submitBtnText</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>skipSubmit</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>submitBtnStyle</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<!-- Funkciq koqto se izpylnqva predi validaciite -->
		<attribute>
			<name>onsubmit</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>enctype</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<!--
				Dopylnitelna validaciq (razlichna ot standartnite.Pr. za zaqvleniq-obuchenie trqbva da se vyvede base university, koeto se razbira 4e e vyvedeno
				ot edin skrit input box i ne moje da se standartizira!). Izraza trqbva da se iz4islqva do boolean i NE TRQBVA DA IMA ";" nakraq
			-->
			<name>additionalvalidation</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<!-- url-to na back button-a -->
			<name>backurl</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<!-- url-to na print button-a -->
			<name>printurl</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<!-- shte ukazva dali da se slaga print button-a ili ne -->
			<name>addprintbutton</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<!-- dali da propuska chertaeneto na submit buttonite -->
			<name>skipsubmitbuttons</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<!-- ajax koito trqbva da se izpylnqva v slu4ai 4e formata e ajax i ne se submitva -->
			<name>ajax</name>
			<required>false</required>
		</attribute>
		<attribute>
			<name>addGenerateButton</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
            <name>additionalButtons</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>additionalButtonsVisible</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>modelAttribute</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <dynamic-attributes>true</dynamic-attributes>
	</tag>
	
